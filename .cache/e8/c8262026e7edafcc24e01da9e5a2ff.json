lueY/y].map(Math.round);\n\t\t\ttravelContainer.style.backgroundColor = `rgb(${rY}, ${gY}, ${bY})`;\n\t\t});\n\t}\n\tonLeave() { \n\t}\n\tonEnterCompleted() {\n\t\t// make back button visible\n\t\tlet movBack = document.querySelector(\".movBack\");\n\t\tmovBack.style.opacity = 1;\n\t }\n\tonLeaveCompleted() { }\n}\n\n// export renderer\nexport default TravelRenderer;"},"lineCount":null}},"error":null,"hash":"d2cd93e580f34002a2b629d771544f99","cacheData":{"env":{}}}