\t\t\tmiscContent.style.backgroundColor = `rgb(${rY}, ${gY}, ${bY})`;\n\t\t});\n\t}\n\tonLeave() {\n\t\t// End Rellax and reset parallax elements to their original positions\n\t\t// rellaxMisc.destroy();\n\t}\n\tonEnterCompleted() {\n\t\t// make back button visible\n\t\tlet movBack = document.querySelector(\".movBack\");\n\t\tmovBack.style.opacity = 1;\n\t}\n\tonLeaveCompleted() {}\n}\n\n// export renderer\nexport default MiscRenderer;\n"},"lineCount":null}},"error":null,"hash":"32b9bcb8be3a438d2c1d75a981c4dc35","cacheData":{"env":{}}}